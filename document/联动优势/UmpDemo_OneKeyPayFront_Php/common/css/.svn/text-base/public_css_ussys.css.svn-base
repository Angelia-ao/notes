      body{padding:0;margin:0;font-size:12px;text-align:center;font-family:arial,宋体;}
      img{border:0;}
      #HEAD,#HEADA{width:586px;padding:11px 0 5px 0;margin:0 auto;border-bottom:3px #ddd solid;}
